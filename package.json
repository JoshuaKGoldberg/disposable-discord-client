		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier \"**/*\" --ignore-unknown",
		"lint": "eslint . .*js --max-warnings 0",
		"lint:knip": "knip",
