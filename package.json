		"typescript": "^5.3.3",
		"yaml-eslint-parser": "^1.2.2"
	},
	"peerDependencies": {
		"discord.js": "^14.14.1"
	},
	"packageManager": "pnpm@8.7.0",
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"provenance": true
	}
